{% extends 'base.html.twig' %}
{% block body %}
  <section class="faq-area faq-about bg_cover pt-115 pb-120">
    <div class="container">
      <div class="row justify-content-center">
        <div class="col-lg-8">
          <div class="section-title text-center">
            <span>3Dtech</span>
            <h3 class="title">Inscription </h3>
          </div>
        </div>
      </div>
    </div>
  </section>
  <div class="faq-box-area faq-box-about">
    <div class="container">
      <div class="row">
        <div class="col-lg-12">
          <div class="faq-box white-bg">
            <div class="row">
              <div class="col-lg-12">
                <div class="faq-form">
                  <div class="faq-title">
                    <h4 class="title">Inscription</h4>
                  </div>
                  {{ form_start(form, {'attr': { 'id' : 'registration-form'}}) }}
                  <div class="input-box mt-1400" style="padding-bottom: 5px">
                    {{ form_row(form.client.type) }}
                  </div>
                  <div class="input-box mt-20">
                    <div class="custom-error">
                      {{ form_errors(form.email) }}
                    </div>
                    {{ form_widget(form.email) }}
                  </div>
                  <div class="input-box mt-20">
                    <div class="custom-error">
                      {{ form_errors(form.plainPassword.children['first']) }}
                    </div>
                    {{ form_widget(form.plainPassword.children['first']) }}
                  </div>

                  <div class="input-box mt-20">
                    <div class="custom-error">
                      {{ form_errors(form.plainPassword.children['second']) }}
                    </div>
                    {{ form_widget(form.plainPassword.children['second']) }}
                  </div>
                  <div class="input-box mt-20">
                    <div class="custom-error">
                      {{ form_errors(form.client.lastName) }}
                    </div>
                    {{ form_widget(form.client.lastName) }}
                  </div>
                  <div class="input-box mt-20">
                    <div class="custom-error">
                      {{ form_errors(form.client.mobilePhone) }}
                    </div>
                    <div>
                      {{ form_widget(form.client.mobilePhone) }}
                    </div>
                  </div>
                  <div class="input-box mt-20">
                    <div class="custom-error">
                      {{ form_errors(form.client.firstName) }}
                    </div>
                    <div>
                      {{ form_widget(form.client.firstName) }}
                    </div>
                  </div>

                  </div>
                <div class="input-box mt-20">
                  {{ form_row(form.submit, {'attr':{'class': 'main-btn btn-lg'}}) }}
                </div>
                  {{ form_row(form._token) }}
                  {{ form_start(form) }}
                </div>

              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  {% include 'common/brand_section.html.twig' %}
  {% include 'common/subcribe_part.html.twig' %}

{% endblock %}
